var csv = require('fast-csv');
var fs = require('fs');
var stream = fs.createReadStream("salesHistory.csv");

var itemArr = [];
 
csv
 .fromStream(stream,{delimiter: ';'}, {headers : ["Day", "Date", "stockItem", "NoSold", "Sales", "Price"]})
 .on("data", function(data){
    // console.log(data);
	itemArr.push(data);
 })
 .on("end", function(){
     console.log("done");
	console.log(itemArr);
 });
 
