<div id="page-wrapper" >
    <div id="page-inner">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <h1>Nelisa Spaza Inc</h1>
                <p>Welcome to Nelisa Spaza Inc Inventory System</p>
            </div>
        </div>

        <hr />
        <div><h3 class = 'text-center'>{{title}}</h3></div>
        <div class="row">

            <div class="col-md-12 col-sm-12 col-xs-12">
                <table class="table">
                    <tr>
                        <th>Name</th>
                        <th>Value</th>
                    </tr>
                    {{#each prod}}
                    <tr>
                            <td>{{name}}</td>
                            <td>{{amt}}</td>
                    </tr>
                    {{/each}}
                </table>
            </div>

        </div>

    </div>
</div>

