exports.milkList = function(itemArr, prodType){ 
	var milkArr = [];
	var total = 0;
        for (var i = 0; i < itemArr.length; i++){
                if(itemArr[i][2] === prodType){
                        milkArr.push(itemArr[i]);
                }
        }

        for (var x = 0; x < milkArr.length; x++){
		total += parseInt(milkArr[x][3]);
	}

	console.log("-----MILK 1L-----");
	console.log("Total Milk Sold: " +total);
	var milkCash = total * 10.00;
	console.log("Total Milk Turnover: R" +milkCash +"\n");
	

	return {
	  productName : "",
	  qty :
	  total :
	};	
}
