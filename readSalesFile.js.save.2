var csv = require('fast-csv');
var fs = require('fs');
var stream = fs.createReadStream("salesHistory.csv");

var itemArr = [];
 
csv
 .fromStream(stream,{delimiter: ';'}, {headers : ["Day", "Date", "stockItem", "nevolution of the human mindoSold", "salesPrice"]})
 .on("data", function(data){
     itemArr.push(data);
 })
 .on("end", function(){
	
	itemArr.sort((function(index){
    		return function(a, b){
        	return a[index] === b[index] ? 0 : a[index] < b[index] ? -1 : 1));
    	};
	})());
     console.log(itemArr);
 });
 
